@model AMS.Areas.DashBoard.Models.ViewModels.AccomodationTypeModels
   @if(Model != null && Model.accomodationTypes!= null && Model.accomodationTypes.Count()>0)
   {
    <table class="table table-striped">
        <thead>
            <tr>
                <th scope="col">Name</th>
                <th scope="col">Description</th>
                <th scope="col">action</th>
              
            </tr>
        </thead>
        <tbody>
            @foreach(var item in Model.accomodationTypes)
            {
             <tr>
                 <td>@item.Name</td>
                    <td>@item.Description</td>
                    <td>
                        <button   data-href="@Url.Action("Action","AccomodationTypes",new { id=item.ID})" type="button" data-toggle="modal" data-target="#ActionModal" class="data-btn btn btn-success"> <i class="fas fa-pencil-alt mr-1"></i>Edit</button>
                        <button   data-href="@Url.Action("Delete","AccomodationTypes",new { id=item.ID})"type="button" data-toggle="modal" data-target="#ActionModal" class=" data-btn btn-danger"><i class="fas fa-trash-alt mr-1"></i>Delete</button>
                    </td>
                </tr>
            }
        </tbody>
    </table>

}
   else
{
<div class="alert alert-warning" role="alert">
 sorry no rRecord Exist
</div>
}